#three .bg{
    z-index: 1;
    background: -moz-linear-gradient(top, rgba(66,38,81,0.1) 0%, rgba(66,38,81,0.1) 63%, rgba(66,38,81,0.5) 100%);
    background: -webkit-linear-gradient(top, rgba(66,38,81,0.1) 0%,rgba(66,38,81,0.1) 63%,rgba(66,38,81,0.5) 100%);
    background: linear-gradient(to bottom, rgba(66,38,81,0.1) 0%,rgba(66,38,81,0.1) 63%,rgba(66,38,81,0.5) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1a422651', endColorstr='#80422651',GradientType=0 );
}
#three .carousel-caption{
    color: #452859;
    text-align: left;
    left: 0;
    top: 0;
    right: 0;
    padding-top: 0;
    padding-bottom: 0;
}
#three .carousel .carousel-item{
    background: no-repeat top center;
    min-height: 480px;
    background-size: cover;
}
.carousel-indicators li{
    width: 9px;
    height: 9px;
    border-radius: 4px;
    margin-right: 5px;
    margin-left: 5px;
    border: 2px solid #ffd248;
}
.carousel-indicators .active {
    background-color: #ffd248;
}
.carousel-control-next:focus, .carousel-control-next:hover, .carousel-control-prev:focus, .carousel-control-prev:hover{
    color: #ffd248;
}
.carousel-control-next, .carousel-control-next, .carousel-control-prev, .carousel-control-prev{
    color: #ffd248;
}
.carousel-control-next, .carousel-control-prev{
    width: 5%;
    opacity: 1;
    font-size: 50px;
}